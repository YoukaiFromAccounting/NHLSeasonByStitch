<script>

document.addEventListener('DOMContentLoaded', function() {
    const stars = document.querySelectorAll('#rating-stars .star');
    const ratingContainer = document.getElementById('rating-container');
    const logoId = ratingContainer.getAttribute('data-logo-id');

    let selectedRating = parseFloat(document.getElementById('average-rating').dataset.rating) || 0;

    highlightStars(selectedRating); // <-- Important! Highlight right away

    stars.forEach(star => {
        star.addEventListener('mouseover', function() {
            const val = this.getAttribute('data-value');
            highlightStars(val);
        });

        star.addEventListener('mouseout', function() {
            highlightStars(selectedRating);
        });

        star.addEventListener('click', function() {
            const rating = this.getAttribute('data-value');
            selectedRating = rating;
            sendRating(rating);
        });
    });

    function highlightStars(rating) {
        stars.forEach(star => {
            const val = parseFloat(star.getAttribute('data-value'));
            const fg = star.querySelector('.star-fg');

            if (val <= rating) {
                fg.style.width = "100%";
            } else if (val - 1 < rating && val > rating) {
                fg.style.width = ((rating - Math.floor(rating)) * 100) + "%";
            } else {
                fg.style.width = "0";
            }
        });
    }

function sendRating(rating) {
    const xhr = new XMLHttpRequest();
    xhr.open('POST', '/rate_logo.php', true);
    xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    xhr.onload = function () {
        if (xhr.status === 200) {
            const response = JSON.parse(xhr.responseText);
            if (response.success) {
                document.getElementById('average-rating').textContent = response.new_average + " / 10";
                document.getElementById('rating-count').textContent =
                    "(" + response.new_count + " vote" + (response.new_count == 1 ? "" : "s") + ")";

                const message = document.getElementById('vote-message');
                message.textContent = "Thanks! Your vote was recorded.";
                message.style.display = "block";

                // Hide after 3 seconds
                setTimeout(() => {
                    message.style.display = "none";
                }, 3000);
            } else {
                alert('Error: ' + response.message);
            }
        }
    };
    xhr.send('logo_id=' + encodeURIComponent(logoId) + '&rating=' + encodeURIComponent(rating));
}
});
</script>
